<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <title>Express</title>
  </head>
  <body>
    <script src="https://unpkg.com/vue@3/dist/vue.global.js"></script>
    <div id="app">
      <hello-world msg="Message from backend" />
    </div>
    {{ #devFiles }}
      <script type="module" src="{{ devFiles.vite }}"></script>
      <script type="module" src="{{ devFiles.main }}"></script>
    {{ /devFiles }}
    {{ #prodFiles }}
      <link rel="stylesheet" href="{{ prodFiles.css }}" />
      <script type="module" src="{{ prodFiles.js }}"></script>
    {{ /prodFiles }}
  </body>
</html>
